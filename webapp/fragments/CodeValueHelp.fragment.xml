<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog id="_IDGenSelectDialog1"
		noDataText="No Codes"
		title="Select code"
		search=".onCodeSearch"
		multiSelect="false"
		confirm=".onCodeValueHelpDialogClose"
		cancel=".onCodeValueHelpDialogClose"
		showClearButton="false"
		items="{
			path: '/F4_CodeSet',
			sorter: {
				path: 'Doc_No',
				descending: false
			}
		}">
		<StandardListItem id="_IDGenStandardListItem1" title="{Doc_No}" iconDensityAware="false" iconInset="false" type="Active" />
	</SelectDialog>
</core:FragmentDefinition>