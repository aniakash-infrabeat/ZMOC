<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		noDataText="No Codes"
		title="Select code"
		search=".onCodeSearch"
		multiSelect="false"
		confirm=".onCodeValueHelpDialogClose"
		cancel=".onCodeValueHelpDialogClose"
		showClearButton="false"
		items="{
			path: '/F4_CodeSet',
			sorter: {
				path: 'Code',
				descending: false
			}
		}">
		<StandardListItem title="{Code}" iconDensityAware="false" iconInset="false" type="Active" />
	</SelectDialog>
</core:FragmentDefinition>